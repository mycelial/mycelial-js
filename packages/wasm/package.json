{
  "name": "@mycelial/wasm",
  "version": "0.2.1",
  "description": "The Mycelial WASM Interface",
  "license": "Apache-2.0",
  "author": "Mycelial <hello@mycelial.com>",
  "module": "./dist/module/index.js",
  "keywords": [],
  "files": [
    "dist/module/index_bg.wasm",
    "dist/module/index.js",
    "dist/module/index_bg.js",
    "dist/module/index.d.ts",
    "dist/nodejs/index_bg.wasm",
    "dist/nodejs/index.js",
    "dist/nodejs/index_bg.js",
    "dist/nodejs/index.d.ts"
  ],
  "scripts": {
    "build": "npm run build:nodejs && npm run build:module",
    "build:nodejs": "wasm-pack build -d dist/nodejs --out-name index -s mycelial -t nodejs",
    "build:module": "wasm-pack build -d dist/module --out-name index -s mycelial -t bundler"
  },
  "exports": {
    "node": "./dist/nodejs/index.js",
    "import": "./dist/module/index.js"
  },
  "types": "./dist/module/index.d.ts",
  "sideEffects": false
}